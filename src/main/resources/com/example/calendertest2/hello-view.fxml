<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="vContainer" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.calendertest2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox fx:id="menuHbox" alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <ImageView fx:id="upArrow" fitHeight="47.0" fitWidth="46.0" onMouseClicked="#incrementMonth" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/upArrow.png" />
               </image>
            </ImageView>
            <ImageView fx:id="downArrow" fitHeight="47.0" fitWidth="46.0" layoutX="10.0" layoutY="37.0" onMouseClicked="#decrementMonth" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/downArrow.png" />
               </image>
            </ImageView>
            <Label fx:id="monthLabel" alignment="CENTER" prefHeight="100.0" prefWidth="153.0" text="Month">
               <font>
                  <Font name="SimSun" size="48.0" />
               </font>
            </Label>
            <Label fx:id="yearLabel" alignment="CENTER" prefHeight="100.0" prefWidth="153.0" text="Year">
             <font>
                 <Font name="SimSun" size="48.0" />
             </font>
         </Label>
         </children>
      </HBox>
      <GridPane fx:id="calenderGrid" prefHeight="431.0" prefWidth="700.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="84.0" minHeight="0.0" prefHeight="26.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="173.0" minHeight="10.0" prefHeight="86.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="163.0" minHeight="10.0" prefHeight="87.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="116.0" minHeight="10.0" prefHeight="84.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="109.0" minHeight="10.0" prefHeight="79.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="109.0" minHeight="10.0" prefHeight="71.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
   </children>
</VBox>
